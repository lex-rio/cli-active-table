{
  "name": "cli-active-table",
  "version": "1.1.5",
  "description": "cli tool that helps you render data in command line interface",
  "main": "dist/active-table.js",
  "scripts": {
    "build": "rm -rf dist & tsc",
    "test": "tsx tests/index.ts",
    "start": "tsx examples/index.ts",
    "examples:multiple": "tsx examples/multiple.ts",
    "examples:issues": "tsx examples/issues.ts",
    "examples:crypto": "tsx examples/crypto.ts",
    "examples:books": "tsx examples/books.ts",
    "examples:launches": "tsx examples/launches.ts",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lex-rio/cli-active-table.git"
  },
  "keywords": [
    "cli",
    "table"
  ],
  "bin": {
    "cli-active-table": "bin/cli.js"
  },
  "types": "dist/active-table.d.ts",
  "author": "lex-rio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lex-rio/cli-active-table/issues"
  },
  "homepage": "https://github.com/lex-rio/cli-active-table#readme",
  "peerDependencies": {
    "typescript": ">=5.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
